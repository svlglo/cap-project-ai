{
  "name": "cap-project",
  "version": "1.0.0",
  "description": "A simple CAP project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js-community/odata-v2-adapter": "^1.14.4",
    "@cap-js/hana": "^1",
    "@sap-ai-sdk/langchain": "^1.14.0",
    "@sap/cds": "^8",
    "@sap/hana-client": "^2.25.22",
    "@sap/xssec": "^4",
    "express": "^4"
  },
  "devDependencies": {
    "@cap-js-community/odata-v2-adapter": "^1.14.4",
    "@cap-js/cds-types": "^0.10.0",
    "@cap-js/sqlite": "^1",
    "@sap-ai-sdk/langchain": "^1.14.0",
    "@sap/cds-dk": "^8"
  },
  "scripts": {
    "start": "cds-serve",
    "hybrid": "cds watch --profile hybrid"
  },
  "cds": {
    "requires": {
      "[production]": {
        "db": "hana",
        "auth": "xsuaa"
      },
      "[hybrid]": {
        "custom-service:default_aicore": {
          "binding": {
            "type": "cf",
            "apiEndpoint": "https://api.cf.us10-001.hana.ondemand.com",
            "org": "globant-ssa-dev",
            "space": "DEV",
            "instance": "default_aicore",
            "key": "default_aicore-key",
            "resolved": false
          },
          "vcap": {
            "name": "custom-service:default_aicore"
          }
        }
      }
    },
    "sql": {
      "native_hana_associations": false
    }
  }
}
